import numpy as np
import psi4
psi4.core.be_quiet()
import mesp

geom = """
    O
    H 1 1
    H 1 1 2 104.5
    symmetry c1
"""

bas = "sto-3g"

psi4.set_options({
    'basis':bas,
    'scf_type':'pk',
    'mp2_type':'conv',
    'freeze_core':'false',
    'e_convergence':1e-12,
    'd_convergence':1e-12})

mol = mesp.Molecule('H2O',geom,bas)

mesp.do_scf(mol)

mesp.do_mp2(mol)

mesp.do_ccsd(mol)
